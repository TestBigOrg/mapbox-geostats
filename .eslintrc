{
  "extends": "airbnb-base/legacy",
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "vars-on-top": ["off"],
    "func-names": ["off"],
    "no-use-before-define": ["off"],
    "consistent-return": ["off"],
    "no-param-reassign": ["off"],
    "no-underscore-dangle": ["off"],
    "no-new": ["off"]
  }
}
